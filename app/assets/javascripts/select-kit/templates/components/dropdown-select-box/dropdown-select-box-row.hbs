{{#if icons}}
  <div class="icons">
    <span class="selection-indicator"></span>
    {{#each icons as |icon|}}
      {{d-icon icon translatedtitle=(dasherize title)}}
    {{/each}}
  </div>
{{/if}}

<div class="texts">
  <span class="name">{{{label}}}</span>
  <span class="desc">{{{description}}}</span>
</div>
